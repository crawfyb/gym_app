<% @members.each do |member| %>

<div class="member">
  <p>Name: <%= member.pretty_name() %> </p>
  <p><a href="/members/<%=member.id%>">View details</a></p>
  <form action="/members/<%=member.id%>/delete" method="POST">
        <input class="btn btn--danger" type="submit" value="delete member" />
  </form>
  <hr />
</div>

<%end%>
